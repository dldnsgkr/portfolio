    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tanstack/router-devtools": "^1.98.4",
    "@tanstack/router-plugin": "^1.98.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
